	local s = Instance.new("UICorner")
	local bt = {}
	local bu = false;
	local bv = false;
	local bw = ""
	local bs = bs or Enum.KeyCode.F1;
	local bx = string.gsub(tostring(bs), "Enum.KeyCode.", "")
	local by = Instance.new("ScreenGui")
	by.Name = "VVV"
