function InfAb()
    local player = game:GetService("Players").LocalPlayer
    local rootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")

    if InfAbility then
        if not (rootPart and rootPart:FindFirstChild("Agility")) then
            local inf = Instance.new("ParticleEmitter")
            inf.Archivable, inf.Enabled, inf.LockedToPart = true, true, true
            inf.Acceleration, inf.EmissionDirection = Vector3.new(0, 0, 0), Enum.NormalId.Top
            inf.Lifetime, inf.LightInfluence, inf.Name = NumberRange.new(0, 0), 0, "Agility"
            inf.Rate = 500
            inf.Size = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0),
                NumberSequenceKeypoint.new(1, 4)
            })
            inf.RotSpeed, inf.Rotation, inf.Speed = NumberRange.new(9999, 99999), NumberRange.new(0, 0), NumberRange.new(30, 30)
            inf.SpreadAngle, inf.Texture, inf.VelocityInheritance = Vector2.new(0, 0), "", 0
            inf.ZOffset, inf.Transparency, inf.Color = 2, NumberSequence.new(0), ColorSequence.new(Color3.fromRGB(0, 0, 0), Color3.fromRGB(0, 0, 0))
            inf.Parent = rootPart
        end
    else
        local agility = rootPart and rootPart:FindFirstChild("Agility")
        if agility then
            agility:Destroy()
        end
    end
end

local LocalPlayer = game:GetService('Players').LocalPlayer
local originalstam = LocalPlayer.Character.Energy.Value

function infinitestam()
    LocalPlayer.Character.Energy.Changed:Connect(function()
        if InfiniteEnergy then
            LocalPlayer.Character.Energy.Value = originalstam
        end 
    end)
end

spawn(function()
    while wait(0.1) do
        if InfiniteEnergy then
            originalstam = LocalPlayer.Character.Energy.Value
            if not connected then
                infinitestam()
                connected = true
            end
        elseif connected then
            LocalPlayer.Character.Energy.Changed:Disconnect()
            connected = false
        end
    end
end)

function NoDodgeCool()
    if nododgecool then
        local localPlayer = game:GetService("Players").LocalPlayer
        local dodgeAbility = localPlayer.Character and localPlayer.Character.Dodge

        if dodgeAbility then
            for _, func in pairs(getgc()) do
                if typeof(func) == "function" and getfenv(func).script == dodgeAbility then
                    for i, value in pairs(getupvalues(func)) do
                        if tostring(value) == "0.1" then
                            repeat
                                wait(0.1)
                                setupvalue(func, i, 0)
                            until not nododgecool
                        end
                    end
                end
            end
        end
    end
end

function fly()
    local player = game:GetService("Players").LocalPlayer
    local mouse = player:GetMouse()
    local torso = player.Character:WaitForChild("HumanoidRootPart")
    local flying = false
    local speedSet = 25
    local keys = {a = false, d = false, w = false, s = false}
    local speed = 0
    
    local function start()
        local pos = Instance.new("BodyPosition", torso)
        local gyro = Instance.new("BodyGyro", torso)
        pos.Name = "EPIXPOS"
        pos.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        pos.Position = torso.Position
        gyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        gyro.CFrame = torso.CFrame
    
        repeat
            wait()
            player.Character.Humanoid.PlatformStand = true
            local new = gyro.CFrame - gyro.CFrame.p + pos.Position
    
            if not keys.w and not keys.s and not keys.a and not keys.d then
                speed = 1
            end
    
            if keys.w then
                new = new + workspace.CurrentCamera.CoordinateFrame.LookVector * speed
                speed = speed + speedSet
            end
    
            if keys.s then
                new = new - workspace.CurrentCamera.CoordinateFrame.LookVector * speed
                speed = speed + speedSet
            end
    
            if keys.d then
                new = new * CFrame.new(speed, 0, 0)
                speed = speed + speedSet
            end
    
            if keys.a then
                new = new * CFrame.new(-speed, 0, 0)
                speed = speed + speedSet
            end
    
            speed = math.min(speed, speedSet)
            pos.Position = new.p
    
            if keys.w then
                gyro.CFrame = workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad(speed * 15), 0, 0)
            elseif keys.s then
                gyro.CFrame = workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(math.rad(speed * 15), 0, 0)
            else
                gyro.CFrame = workspace.CurrentCamera.CoordinateFrame
            end
        until not flying
    
        gyro:Destroy()
        pos:Destroy()
        flying = false
        player.Character.Humanoid.PlatformStand = false
        speed = 0
    end
    
    local e1 = mouse.KeyDown:Connect(function(key)
        if not torso or not torso.Parent or not torso:IsA("BasePart") then
            flying = false
            e1:Disconnect()
            e2:Disconnect()
            return
        end
    
        if key == "w" then
            keys.w = true
        elseif key == "s" then
            keys.s = true
        elseif key == "a" then
            keys.a = true
        elseif key == "d" then
            keys.d = true
        end
    end)
    
    local e2 = mouse.KeyUp:Connect(function(key)
        if key == "w" then
            keys.w = false
        elseif key == "s" then
            keys.s = false
        elseif key == "a" then
            keys.a = false
        elseif key == "d" then
            keys.d = false
        end
    end)
    
    start()
    
end

function Click()
    game:GetService'VirtualUser':CaptureController()
    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
end

function AutoHaki()
    if not game:GetService("Players").LocalPlayer.Character:FindFirstChild("HasBuso") then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
    end
end

function UnEquipWeapon(Weapon)
    if game.Players.LocalPlayer.Character:FindFirstChild(Weapon) then
        _G.NotAutoEquip = true
        wait(.5)
        game.Players.LocalPlayer.Character:FindFirstChild(Weapon).Parent = game.Players.LocalPlayer.Backpack
        wait(.1)
        _G.NotAutoEquip = false
    end
end

function EquipWeapon(ToolSe)
    if not _G.NotAutoEquip then
        if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
            Tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
            wait(.1)
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(Tool)
        end
    end
end

function GetDistance(target)
    return math.floor((target.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
end

function BTP(p)
    pcall(function()
        local localPlayer = game.Players.LocalPlayer
        local rootPart = localPlayer.Character.HumanoidRootPart
        local humanoid = localPlayer.Character.Humanoid

        if (p.Position - rootPart.Position).Magnitude >= 1500 and not Auto_Raid and humanoid.Health > 0 then
            repeat
                wait()
                rootPart.CFrame = p
                wait(0.05)
                game.Players.LocalPlayer.Character.Head:Destroy()
                rootPart.CFrame = p
            until (p.Position - rootPart.Position).Magnitude < 1500 and humanoid.Health > 0
        end
    end)
end

function TelePPlayer(P)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = P
end

function TP1(Pos)
    local humanoid = game.Players.LocalPlayer.Character.Humanoid
    local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart

    local Distance = (Pos.Position - rootPart.Position).Magnitude
    if humanoid.Sit then humanoid.Sit = false end

    local tween = game:GetService("TweenService"):Create(rootPart, TweenInfo.new(Distance / 210, Enum.EasingStyle.Linear), { CFrame = Pos })
    tween:Play()

    if Distance <= 250 or _G.StopTween then
        tween:Cancel()
        rootPart.CFrame = Pos
    end
end

function TP(Pos)
    local Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    local Speed

    if Distance < 10 then
        Speed = 10000
    elseif Distance < 25 then
        Speed = 5000
    elseif Distance < 50 then
        Speed = 2000
    elseif Distance < 150 then
        Speed = 800
    elseif Distance < 250 then
        Speed = 600
    elseif Distance < 500 then
        Speed = 450
    elseif Distance < 750 then
        Speed = 400
    elseif Distance >= 1000 then
        Speed = 350
    end

    local TweenService = game:GetService("TweenService")
    local HumanoidRootPart = game.Players.LocalPlayer.Character.HumanoidRootPart

    local tweenInfo = TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear)
    local goal = {}
    goal.CFrame = Pos

    local tween = TweenService:Create(HumanoidRootPart, tweenInfo, goal)
    tween:Play()

    _G.Clip = true
    tween.Completed:Wait()  -- Wait until the tween completes
    _G.Clip = false
end



function topos(Pos)
    local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
    local Distance = (Pos.Position - rootPart.Position).Magnitude

    if game.Players.LocalPlayer.Character.Humanoid.Sit then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    end

    local tween = game:GetService("TweenService"):Create(rootPart, TweenInfo.new(Distance / 210, Enum.EasingStyle.Linear), { CFrame = Pos })
    tween:Play()

    if Distance <= 250 or _G.StopTween then
        tween:Cancel()
        rootPart.CFrame = Pos
        _G.Clip = not _G.StopTween
    end
end

function TPB(CFgo)
	local tween_s = game:GetService("TweenService")
	local seatCFrame = game:GetService("Workspace").Boats.PirateBrigade.VehicleSeat.CFrame
	local distance = (seatCFrame.Position - CFgo.Position).Magnitude
	local info = TweenInfo.new(distance / 300, Enum.EasingStyle.Linear)

	local tween = tween_s:Create(game:GetService("Workspace").Boats.PirateBrigade.VehicleSeat, info, { CFrame = CFgo })
	tween:Play()

	return { Stop = function() tween:Cancel() end }
end

function TPP(CFgo)
	local player = game.Players.LocalPlayer
	local character = player.Character

	if not character or character:WaitForChild("Humanoid").Health <= 0 then
		return { Stop = function() end }
	end

	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	local distance = (humanoidRootPart.Position - CFgo.Position).Magnitude
	local tweenService = game:GetService("TweenService")
	local info = TweenInfo.new(distance / 325, Enum.EasingStyle.Linear)

	local tween = tweenService:Create(humanoidRootPart, info, { CFrame = CFgo })
	tween:Play()

	return { Stop = function() tween:Cancel() end }
end

getgenv().ToTargets = function(p)
    task.spawn(function()
        pcall(function()
            local localPlayer = game:GetService("Players").LocalPlayer
            local character = localPlayer.Character

            if localPlayer:DistanceFromCharacter(p.Position) <= 250 then
                character.HumanoidRootPart.CFrame = p
            elseif not character:FindFirstChild("Root") then
                local root = Instance.new("Part", character)
                root.Size = Vector3.new(1, 0.5, 1)
                root.Name = "Root"
                root.Anchored = true
                root.Transparency = 1
                root.CanCollide = false
                root.CFrame = character.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
            end

            local distance = (character.HumanoidRootPart.Position - p.Position).Magnitude
            local tweenService = game:GetService("TweenService")
            local tweenInfo = TweenInfo.new(distance / 300, Enum.EasingStyle.Linear)

            local success, tween = pcall(function()
                local newTween = tweenService:Create(character.Root, tweenInfo, { CFrame = p })
                newTween:Play()
                return newTween
            end)

            if not success then
                return tween
            end

            character.Root.CFrame = character.HumanoidRootPart.CFrame

            if success and character:FindFirstChild("Root") then
                pcall(function()
                    if distance >= 20 then
                        spawn(function()
                            pcall(function()
                                if (character.Root.Position - character.HumanoidRootPart.Position).Magnitude > 150 then
                                    character.Root.CFrame = character.HumanoidRootPart.CFrame
                                else
                                    character.HumanoidRootPart.CFrame = character.Root.CFrame
                                end
                            end)
                        end)
                    elseif distance >= 10 and distance < 20 then
                        character.HumanoidRootPart.CFrame = p
                    elseif distance < 10 then
                        character.HumanoidRootPart.CFrame = p
                    end
                end)
            end
        end)
    end)
end

Type = 1
local Pos = CFrame.new(0, PosY, -30)

spawn(function()
    while wait(.1) do
        if Type == 1 then
            Pos = CFrame.new(0, PosY, -30)
        elseif Type == 2 then
            Pos = CFrame.new(30, PosY, 0)
        elseif Type == 3 then
            Pos = CFrame.new(0, PosY, 30)	
        elseif Type == 4 then
            Pos = CFrame.new(-30, PosY, 0)
        end
        -- Use the 'Pos' variable as needed in this script
    end
end)

spawn(function()
    while wait(0.5) do
        Type = (Type % 4) + 1 -- Cycles through 1, 2, 3, 4
    end
end)

