function Hop()
    local PlaceID = game.PlaceId
    local AllIDs, foundAnything, actualHour = {}, "", os.date("!*t").hour
    local function TPReturner()
        local Site = game.HttpService:JSONDecode(game:HttpGet(string.format('https://games.roblox.com/v1/games/%s/servers/Public?sortOrder=Asc&limit=100%s', PlaceID, foundAnything == "" and "" or "&cursor=" .. foundAnything)))
        if Site.nextPageCursor and Site.nextPageCursor ~= "null" and Site.nextPageCursor ~= nil then
            foundAnything = Site.nextPageCursor
        end
        for _, v in pairs(Site.data) do
            if tonumber(v.maxPlayers) > tonumber(v.playing) then
                local Possible = not table.find(AllIDs, tostring(v.id))
                if Possible then
                    table.insert(AllIDs, tostring(v.id))
                    wait()
                    pcall(function()
                        wait()
                        game:GetService("TeleportService"):TeleportToPlaceInstance(PlaceID, tostring(v.id), game.Players.LocalPlayer)
                    end)
                    wait(4)
                end
            end
        end
    end
    local function Teleport()
        while wait() do
            pcall(function()
                TPReturner()
                if foundAnything ~= "" then
                    TPReturner()
                end
            end)
        end
    end
    Teleport()
end

function UpdateIslandESP()
    for _, location in pairs(game:GetService("Workspace")["_WorldOrigin"].Locations:GetChildren()) do
        pcall(function()
            if IslandESP then
                if location.Name ~= "Sea" then
                    local nameEsp = location:FindFirstChild('NameEsp')
                    if not nameEsp then
                        local billboard = Instance.new('BillboardGui', location)
                        billboard.Name = 'NameEsp'
                        billboard.ExtentsOffset = Vector3.new(0, 1, 0)
                        billboard.Size = UDim2.new(1, 200, 1, 30)
                        billboard.Adornee = location
                        billboard.AlwaysOnTop = true

                        local nameLabel = Instance.new('TextLabel', billboard)
                        nameLabel.Font = "GothamBold"
                        nameLabel.FontSize = "Size14"
                        nameLabel.TextWrapped = true
                        nameLabel.Size = UDim2.new(1, 0, 1, 0)
                        nameLabel.TextYAlignment = 'Top'
                        nameLabel.BackgroundTransparency = 1
                        nameLabel.TextStrokeTransparency = 0.5
                        nameLabel.TextColor3 = Color3.fromRGB(7, 236, 240)
                    else
                        nameEsp.TextLabel.Text = (location.Name .. '   \n' .. round((game:GetService('Players').LocalPlayer.Character.Head.Position - location.Position).Magnitude / 3) .. ' Distance')
                    end
                end
            else
                local nameEsp = location:FindFirstChild('NameEsp')
                if nameEsp then
                    nameEsp:Destroy()
                end
            end
        end)
    end
end

function isnil(thing)
    return thing == nil
end
local round = function(n)
    return math.floor(tonumber(n) + 0.5)
end
Number = math.random(1, 1000000)

function UpdatePlayerChams()
    for _, player in pairs(game:GetService('Players'):GetChildren()) do
        pcall(function()
            local character = player.Character
            if character and ESPPlayer then
                local head = character.Head
                local nameEsp = head and head:FindFirstChild('NameEsp' .. Number)

                if head and not nameEsp then
                    local bill = Instance.new('BillboardGui', head)
                    bill.Name = 'NameEsp' .. Number
                    bill.ExtentsOffset = Vector3.new(0, 1, 0)
                    bill.Size = UDim2.new(1, 200, 1, 30)
                    bill.Adornee = head
                    bill.AlwaysOnTop = true

                    local name = Instance.new('TextLabel', bill)
                    name.Font = Enum.Font.GothamSemibold
                    name.FontSize = 'Size14'
                    name.TextWrapped = true
                    name.Text = (player.Name .. ' \n' .. round((game.Players.LocalPlayer.Character.Head.Position - head.Position).Magnitude / 3) .. ' Distance')
                    name.Size = UDim2.new(1, 0, 1, 0)
                    name.TextYAlignment = 'Top'
                    name.BackgroundTransparency = 1
                    name.TextStrokeTransparency = 0.5
                    name.TextColor3 = player.Team == game.Players.LocalPlayer.Team and Color3.new(0, 255, 0) or Color3.new(255, 0, 0)
                elseif nameEsp then
                    nameEsp.TextLabel.Text = (player.Name .. ' | ' .. round((game.Players.LocalPlayer.Character.Head.Position - head.Position).Magnitude / 3) .. ' Distance\nHealth: ' .. round(character.Humanoid.Health * 100 / character.Humanoid.MaxHealth) .. '%')
                end
            elseif player.Character and player.Character.Head:FindFirstChild('NameEsp' .. Number) then
                player.Character.Head:FindFirstChild('NameEsp' .. Number):Destroy()
            end
        end)
    end
end

function UpdatePlayerChams()
    for _, player in pairs(game:GetService('Players'):GetChildren()) do
        pcall(function()
            local character = player.Character
            if character and ESPPlayer then
                local head = character.Head
                local nameEsp = head and head:FindFirstChild('NameEsp' .. Number)

                if head and not nameEsp then
                    local bill = Instance.new('BillboardGui', head)
                    bill.Name = 'NameEsp' .. Number
                    bill.ExtentsOffset = Vector3.new(0, 1, 0)
                    bill.Size = UDim2.new(1, 200, 1, 30)
                    bill.Adornee = head
                    bill.AlwaysOnTop = true

                    local name = Instance.new('TextLabel', bill)
                    name.Font = Enum.Font.GothamSemibold
                    name.FontSize = 'Size14'
                    name.TextWrapped = true
                    name.Text = (player.Name .. ' \n' .. round((game.Players.LocalPlayer.Character.Head.Position - head.Position).Magnitude / 3) .. ' Distance')
                    name.Size = UDim2.new(1, 0, 1, 0)
                    name.TextYAlignment = 'Top'
                    name.BackgroundTransparency = 1
                    name.TextStrokeTransparency = 0.5
                    name.TextColor3 = player.Team == game.Players.LocalPlayer.Team and Color3.new(0, 255, 0) or Color3.new(255, 0, 0)
                elseif nameEsp then
                    nameEsp.TextLabel.Text = (player.Name .. ' | ' .. round((game.Players.LocalPlayer.Character.Head.Position - head.Position).Magnitude / 3) .. ' Distance\nHealth: ' .. round(character.Humanoid.Health * 100 / character.Humanoid.MaxHealth) .. '%')
                end
            elseif player.Character and player.Character.Head:FindFirstChild('NameEsp' .. Number) then
                player.Character.Head:FindFirstChild('NameEsp' .. Number):Destroy()
            end
        end)
    end
end

function UpdateChestChams() 
    for _, chest in pairs(game.Workspace:GetChildren()) do
        pcall(function()
            if string.find(chest.Name, "Chest") and ChestESP then
                local nameEsp = chest:FindFirstChild('NameEsp' .. Number)
                
                if not nameEsp then
                    local bill = Instance.new('BillboardGui', chest)
                    bill.Name = 'NameEsp' .. Number
                    bill.ExtentsOffset = Vector3.new(0, 1, 0)
                    bill.Size = UDim2.new(1, 200, 1, 30)
                    bill.Adornee = chest
                    bill.AlwaysOnTop = true
                    
                    local name = Instance.new('TextLabel', bill)
                    name.Font = Enum.Font.GothamSemibold
                    name.FontSize = 'Size14'
                    name.TextWrapped = true
                    name.Size = UDim2.new(1, 0, 1, 0)
                    name.TextYAlignment = 'Top'
                    name.BackgroundTransparency = 1
                    name.TextStrokeTransparency = 0.5

                    local chestColorMap = {
                        ["Chest1"] = {Color3.fromRGB(109, 109, 109), "Chest 1"},
                        ["Chest2"] = {Color3.fromRGB(173, 158, 21), "Chest 2"},
                        ["Chest3"] = {Color3.fromRGB(85, 255, 255), "Chest 3"}
                    }

                    local chestColorData = chestColorMap[chest.Name]
                    if chestColorData then
                        name.TextColor3 = chestColorData[1]
                        name.Text = (chestColorData[2] .. ' \n' .. round((game.Players.LocalPlayer.Character.Head.Position - chest.Position).Magnitude / 3) .. ' Distance')
                    end
                else
                    nameEsp.TextLabel.Text = (chest.Name .. '   \n' .. round((game.Players.LocalPlayer.Character.Head.Position - chest.Position).Magnitude / 3) .. ' Distance')
                end
            elseif chest:FindFirstChild('NameEsp' .. Number) then
                chest:FindFirstChild('NameEsp' .. Number):Destroy()
            end
        end)
    end
end

function UpdateDevilChams() 
    for _, devilFruit in pairs(game.Workspace:GetChildren()) do
        pcall(function()
            if DevilFruitESP and string.find(devilFruit.Name, "Fruit") then
                local nameEsp = devilFruit.Handle:FindFirstChild('NameEsp' .. Number)
                
                if not nameEsp then
                    local bill = Instance.new('BillboardGui', devilFruit.Handle)
                    bill.Name = 'NameEsp' .. Number
                    bill.ExtentsOffset = Vector3.new(0, 1, 0)
                    bill.Size = UDim2.new(1, 200, 1, 30)
                    bill.Adornee = devilFruit.Handle
                    bill.AlwaysOnTop = true
                    
                    local name = Instance.new('TextLabel', bill)
                    name.Font = Enum.Font.GothamSemibold
                    name.FontSize = 'Size14'
                    name.TextWrapped = true
                    name.Size = UDim2.new(1, 0, 1, 0)
                    name.TextYAlignment = 'Top'
                    name.BackgroundTransparency = 1
                    name.TextStrokeTransparency = 0.5
                    name.TextColor3 = Color3.fromRGB(255, 255, 255)
                    name.Text = (devilFruit.Name .. ' \n' .. round((game.Players.LocalPlayer.Character.Head.Position - devilFruit.Handle.Position).Magnitude / 3) .. ' Distance')
                else
                    nameEsp.TextLabel.Text = (devilFruit.Name .. '   \n' .. round((game.Players.LocalPlayer.Character.Head.Position - devilFruit.Handle.Position).Magnitude / 3) .. ' Distance')
                end
            elseif devilFruit.Handle:FindFirstChild('NameEsp' .. Number) then
                devilFruit.Handle:FindFirstChild('NameEsp' .. Number):Destroy()
            end
        end)
    end
end

function UpdateFlowerChams() 
	for i,v in pairs(game.Workspace:GetChildren()) do
		pcall(function()
			if v.Name == "Flower2" or v.Name == "Flower1" then
				if FlowerESP then 
					if not v:FindFirstChild('NameEsp'..Number) then
						local bill = Instance.new('BillboardGui',v)
						bill.Name = 'NameEsp'..Number
						bill.ExtentsOffset = Vector3.new(0, 1, 0)
						bill.Size = UDim2.new(1,200,1,30)
						bill.Adornee = v
						bill.AlwaysOnTop = true
						local name = Instance.new('TextLabel',bill)
						name.Font = Enum.Font.GothamSemibold
						name.FontSize = "Size14"
						name.TextWrapped = true
						name.Size = UDim2.new(1,0,1,0)
						name.TextYAlignment = 'Top'
						name.BackgroundTransparency = 1
						name.TextStrokeTransparency = 0.5
						name.TextColor3 = Color3.fromRGB(255, 0, 0)
						if v.Name == "Flower1" then 
							name.Text = ("Blue Flower" ..' \n'.. round((game:GetService('Players').LocalPlayer.Character.Head.Position - v.Position).Magnitude/3) ..' Distance')
							name.TextColor3 = Color3.fromRGB(0, 0, 255)
						end
						if v.Name == "Flower2" then
							name.Text = ("Red Flower" ..' \n'.. round((game:GetService('Players').LocalPlayer.Character.Head.Position - v.Position).Magnitude/3) ..' Distance')
							name.TextColor3 = Color3.fromRGB(255, 0, 0)
						end
					else
						v['NameEsp'..Number].TextLabel.Text = (v.Name ..'   \n'.. round((game:GetService('Players').LocalPlayer.Character.Head.Position - v.Position).Magnitude/3) ..' Distance')
					end
				else
					if v:FindFirstChild('NameEsp'..Number) then
					v:FindFirstChild('NameEsp'..Number):Destroy()
					end
				end
			end   
		end)
	end
end

local function UpdateFruitChams(spawner, espColor)
	for _, tool in pairs(game.Workspace[spawner]:GetChildren()) do
		if tool:IsA("Tool") then
			local espName = 'NameEsp' .. Number
			if RealFruitESP then
				local handle = tool.Handle
				local esp = handle:FindFirstChild(espName)

				if not esp then
					local bill = Instance.new('BillboardGui', handle)
					bill.Name = espName
					bill.ExtentsOffset = Vector3.new(0, 1, 0)
					bill.Size = UDim2.new(1, 200, 1, 30)
					bill.Adornee = handle
					bill.AlwaysOnTop = true

					local name = Instance.new('TextLabel', bill)
					name.Font = Enum.Font.GothamSemibold
					name.FontSize = 'Size14'
					name.TextWrapped = true
					name.Size = UDim2.new(1, 0, 1, 0)
					name.TextYAlignment = 'Top'
					name.BackgroundTransparency = 1
					name.TextStrokeTransparency = 0.5
					name.TextColor3 = espColor
					name.Text = (tool.Name .. '\n' .. round((game.Players.LocalPlayer.Character.Head.Position - handle.Position).Magnitude / 3) .. ' Distance')
				else
					esp.TextLabel.Text = (tool.Name .. ' ' .. round((game.Players.LocalPlayer.Character.Head.Position - handle.Position).Magnitude / 3) .. ' Distance')
				end
			elseif tool.Handle:FindFirstChild(espName) then
				tool.Handle:FindFirstChild(espName):Destroy()
			end
		end
	end
end

function UpdateRealFruitChams()
	UpdateFruitChams("AppleSpawner", Color3.fromRGB(255, 0, 0))
	UpdateFruitChams("PineappleSpawner", Color3.fromRGB(255, 174, 0))
	UpdateFruitChams("BananaSpawner", Color3.fromRGB(251, 255, 0))
end

function UpdateIslandESP()
    for _, location in pairs(game.Workspace["_WorldOrigin"].Locations:GetChildren()) do
        pcall(function()
            if IslandESP and location.Name ~= "Sea" then
                local espName = 'NameEsp'
                local esp = location:FindFirstChild(espName)

                if not esp then
                    local bill = Instance.new('BillboardGui', location)
                    bill.Name = espName
                    bill.ExtentsOffset = Vector3.new(0, 1, 0)
                    bill.Size = UDim2.new(1, 200, 1, 30)
                    bill.Adornee = location
                    bill.AlwaysOnTop = true

                    local name = Instance.new('TextLabel', bill)
                    name.Font = "GothamBold"
                    name.FontSize = "Size14"
                    name.TextWrapped = true
                    name.Size = UDim2.new(1, 0, 1, 0)
                    name.TextYAlignment = 'Top'
                    name.BackgroundTransparency = 1
                    name.TextStrokeTransparency = 0.5
                    name.TextColor3 = Color3.fromRGB(7, 236, 240)
                    name.Text = (location.Name .. '\n' .. round((game.Players.LocalPlayer.Character.Head.Position - location.Position).Magnitude / 3) .. ' Distance')
                else
                    esp.TextLabel.Text = (location.Name .. '   \n' .. round((game.Players.LocalPlayer.Character.Head.Position - location.Position).Magnitude / 3) .. ' Distance')
                end
            elseif location:FindFirstChild('NameEsp') then
                location:FindFirstChild('NameEsp'):Destroy()
            end
        end)
    end
end

local function isnil(thing)
    return thing == nil
end

local function round(n)
    return math.floor(tonumber(n) + 0.5)
end

Number = math.random(1, 1000000)

function UpdatePlayerChams()
	for _, player in pairs(game.Players:GetPlayers()) do
		pcall(function()
			local character = player.Character
			local head = character and character:FindFirstChild("Head")

			if ESPPlayer and head and not head:FindFirstChild('NameEsp'..Number) then
				local bill = Instance.new('BillboardGui', head)
				bill.Name = 'NameEsp'..Number
				bill.ExtentsOffset = Vector3.new(0, 1, 0)
				bill.Size = UDim2.new(1, 200, 1, 30)
				bill.Adornee = head
				bill.AlwaysOnTop = true

				local name = Instance.new('TextLabel', bill)
				name.Font = Enum.Font.GothamSemibold
				name.FontSize = "Size14"
				name.TextWrapped = true
				name.Text = (player.Name ..' | '.. round((game.Players.LocalPlayer.Character.Head.Position - head.Position).Magnitude/3) ..' Distance\nHealth: ' .. round(character.Humanoid.Health * 100 / character.Humanoid.MaxHealth) .. '%')
				name.Size = UDim2.new(1, 0, 1, 0)
				name.TextYAlignment = 'Top'
				name.BackgroundTransparency = 1
				name.TextStrokeTransparency = 0.5

				if player.Team == game.Players.LocalPlayer.Team then
					name.TextColor3 = Color3.new(0, 255, 0)
				else
					name.TextColor3 = Color3.new(255, 0, 0)
				end
			elseif head and head:FindFirstChild('NameEsp'..Number) then
				head:FindFirstChild('NameEsp'..Number):Destroy()
			end
		end)
	end
end

function UpdateChestChams() 
	for _, chest in pairs(game.Workspace:GetChildren()) do
		pcall(function()
			if string.find(chest.Name, "Chest") and ChestESP then
				local espName = 'NameEsp'..Number
				local esp = chest:FindFirstChild(espName)

				if not esp then
					local bill = Instance.new('BillboardGui', chest)
					bill.Name = espName
					bill.ExtentsOffset = Vector3.new(0, 1, 0)
					bill.Size = UDim2.new(1, 200, 1, 30)
					bill.Adornee = chest
					bill.AlwaysOnTop = true

					local name = Instance.new('TextLabel', bill)
					name.Font = Enum.Font.GothamSemibold
					name.FontSize = "Size14"
					name.TextWrapped = true
					name.Size = UDim2.new(1, 0, 1, 0)
					name.TextYAlignment = 'Top'
					name.BackgroundTransparency = 1
					name.TextStrokeTransparency = 0.5

					local chestColor = Color3.new()

					if chest.Name == "Chest1" then
						chestColor = Color3.fromRGB(109, 109, 109)
						name.Text = ("Chest 1\n" .. round((game.Players.LocalPlayer.Character.Head.Position - chest.Position).Magnitude/3) .. ' Distance')
					elseif chest.Name == "Chest2" then
						chestColor = Color3.fromRGB(173, 158, 21)
						name.Text = ("Chest 2\n" .. round((game.Players.LocalPlayer.Character.Head.Position - chest.Position).Magnitude/3) .. ' Distance')
					elseif chest.Name == "Chest3" then
						chestColor = Color3.fromRGB(85, 255, 255)
						name.Text = ("Chest 3\n" .. round((game.Players.LocalPlayer.Character.Head.Position - chest.Position).Magnitude/3) .. ' Distance')
					end

					name.TextColor3 = chestColor
				else
					esp.TextLabel.Text = (chest.Name .. '   \n' .. round((game.Players.LocalPlayer.Character.Head.Position - chest.Position).Magnitude/3) .. ' Distance')
				end
			elseif chest:FindFirstChild('NameEsp'..Number) then
				chest:FindFirstChild('NameEsp'..Number):Destroy()
			end
		end)
	end
end

function UpdateDevilChams() 
