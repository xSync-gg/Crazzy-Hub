                1.08008422e-07,
                0.00823832024
            )
            CFrameMon =
                CFrame.new(
                -16464.9648,
                126.315163,
                1057.79553,
                -0.999545217,
                1.54771485e-08,
                -0.0301560778,
                1.33030991e-08,
                1,
                7.22938864e-08,
                0.0301560778,
                7.18598443e-08,
                -0.999545217
            )
            LevelFarm = 35
        elseif a >= 2525 or _G.SelectMob == "Isle Champion [Lv. 2525]" then
            Mon = "Isle Champion"
            NameQuest = "TikiQuest2"
            LevelQuest = 2
            NameMon = "Isle Champion"
