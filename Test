			cG.BackgroundTransparency = 1;
			cG.BorderSizePixel = 0;
			cG.Position = UDim2.new(0, 1, 0, 1)
			cG.Size = UDim2.new(0, 387, 0, 60)
			r.Name = "UIStroke"
			r.Parent = cG;
			r.ApplyStrokeMode = Enum.ApplyStrokeMode.Border;
			r.Color = _G.Color;
			r.LineJoinMode = Enum.LineJoinMode.Round;
			r.Thickness = 1;
			r.Transparency = 0;
			r.Enabled = true;
			r.Archivable = true;
			s.Parent = cG;
