-----------------------------------------------------------------------------------------------------------------------------Setting Tab
Set:Toggle("Auto Set Spawn Point", true, function(value)
     _G.Set = value
end)
spawn(function()
    while wait() do
        if _G.Set then
            pcall(function()
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
            end)
        end
    end
end)


