-- Scripts:

local function FCSZ_fake_script() -- cac.LocalScript 
	local script = Instance.new('LocalScript', cac)

	function UpdateTime()
		local GameTime = math.floor(workspace.DistributedGameTime+0.5)
		local Hour = math.floor(GameTime/(60^2))%24
		local Minute = math.floor(GameTime/(60^1))%60
		local Second = math.floor(GameTime/(60^0))%60
		script.Parent.Text="Time Elapsed: "..Hour.."h:"..Minute.."m:"..Second.."s"
	end
	
	spawn(function()
		while true do
			UpdateTime()
			wait()
		end
	end)
end
coroutine.wrap(FCSZ_fake_script)()
local function EQILD_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(EQILD_fake_script)()
local function OIKSN_fake_script() -- GodHuman.buy god human 
	local script = Instance.new('LocalScript', GodHuman)

	script.Parent.MouseButton1Click:Connect(function()
	wait(0.3)
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyGodhuman")
	end)
end
coroutine.wrap(OIKSN_fake_script)()
local function RTKVX_fake_script() -- Saguneart.buy saguine art 
	local script = Instance.new('LocalScript', Saguneart)

	script.Parent.MouseButton1Click:Connect(function()
		wait(0.3)
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySanguineArt", true)
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySanguineArt")
	end)
end
coroutine.wrap(RTKVX_fake_script)()
