local a = true
local b = game.Players.LocalPlayer
local c = debug.getupvalues(require(b.PlayerScripts.CombatFramework))
local c = c[2]
function GetCurrentBlade()
    local a = c.activeController
    local a = a.blades[1]
    if not a then
        return
    end
    while a.Parent ~= game.Players.LocalPlayer.Character do
        a = a.Parent
    end
    return a
end
function AttackNoCD()
    local a = c.activeController
    for c = 1, 1 do
        local d =
            require(game.ReplicatedStorage.CombatFramework.RigLib).getBladeHits(
            b.Character,
            {b.Character.HumanoidRootPart},
            60
        )
        local e = {}
        local f = {}
        for a, a in pairs(d) do
            if a.Parent:FindFirstChild("HumanoidRootPart") and not f[a.Parent] then
                table.insert(e, a.Parent.HumanoidRootPart)
                f[a.Parent] = true
            end
        end
        d = e
        if #d > 0 then
            local e = debug.getupvalue(a.attack, 5)
            local f = debug.getupvalue(a.attack, 6)
            local g = debug.getupvalue(a.attack, 4)
            local h = debug.getupvalue(a.attack, 7)
            local i = (e * 798405 + g * 727595) % f
            local j = g * 798405
            (function()
                i = (i * f + j) % 1099511627776
                e = math.floor(i / f)
                g = i - e * f
            end)()
            h = h + 1
            debug.setupvalue(a.attack, 5, e)
            debug.setupvalue(a.attack, 6, f)
            debug.setupvalue(a.attack, 4, g)
            debug.setupvalue(a.attack, 7, h)
            pcall(
                function()
                    for a, a in pairs(a.animator.anims.basic) do
                        a:Play()
                    end
                end
            )
            if b.Character:FindFirstChildOfClass("Tool") and a.blades and a.blades[1] then
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer(
                    "weaponChange",
                    tostring(GetCurrentBlade())
                )
                game.ReplicatedStorage.Remotes.Validator:FireServer(math.floor(i / 1099511627776 * 16777215), h)
                game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("hit", d, c, "")
            end
        end
    end
end
local b
if a then
    b = task.wait
else
    b = wait
end
