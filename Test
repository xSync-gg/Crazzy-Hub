local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
-- Variables:
local input = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
-- Scripts:
local module = {}
function module:CreateUI()
	local Support = game.CoreGui:FindFirstChild("Support")
	if Support then
		Support:Destroy()
	end
	local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
	ScreenGui.Name = "Support"
	local Holder = Instance.new("Frame")
	local Left = Instance.new("Frame")
	local Top = Instance.new("Frame")
	local Right = Instance.new("Frame")
	local Below = Instance.new("Frame")
	Holder.Name = "Holder"
	Holder.Parent = ScreenGui
	Holder.BackgroundColor3 = Color3.fromRGB(56, 185, 255)
	Holder.BackgroundTransparency = 1.000
	Holder.BorderColor3 = Color3.fromRGB(85, 0, 255)
	Holder.Position = UDim2.new(0.333333343, 0, 0.220883533, 0)
	Holder.Size = UDim2.new(0, 50, 0, 50)
	Left.Name = "Left"
	Left.Parent = Holder
	Left.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
	Left.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Left.BorderSizePixel = 0
	Left.Size = UDim2.new(0, 2, 0, 50)
	Top.Name = "Top"
	Top.Parent = Holder
	Top.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
	Top.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Top.BorderSizePixel = 0
	Top.Size = UDim2.new(0, 50, 0, 2)
	Right.Name = "Right"
	Right.Parent = Holder
	Right.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
	Right.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Right.BorderSizePixel = 0
	Right.Position = UDim2.new(0, 49, 0, 0)
	Right.Size = UDim2.new(0, 2, 0, 50)
	Below.Name = "Below"
	Below.Parent = Holder
	Below.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
	Below.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Below.BorderSizePixel = 0
	Below.Position = UDim2.new(0, 0, 0, 50)
	Below.Size = UDim2.new(0, 51, 0, 2)
	return Holder  -- Chỉ cần trả về Holder
end

function module:NewButton(Holder, ID, func1, func2)
	local IconButton = Instance.new("ImageButton", Holder)
	IconButton.Name = "IconButton"
	IconButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	IconButton.BackgroundTransparency = 1.000
	IconButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	IconButton.BorderSizePixel = 0
	IconButton.SelectionImageObject = Holder
	IconButton.Size = UDim2.new(0, 50, 0, 50)
	IconButton.Image = "http://www.roblox.com/asset/?id="..ID	
	local frame = IconButton
	parent = parent or frame
	local dragging = false
	local dragInput, mousePos, framePos
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			mousePos = input.Position
			framePos = parent.Parent.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	frame.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - mousePos
			parent.Parent.Position  = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
		end
	end)
	local button = IconButton
	local originalColor = button.BackgroundColor3
	local hoverColor = Color3.new(1, 0, 0)
	local clickColor = Color3.new(0, 1, 0)
	local dragColor = Color3.new(0, 0, 1)  -- Set the color when the button is dragged
	local TweenService = game:GetService("TweenService")
	local isClicked = false
	local isOpen = false
	local function resetColor()
		local info = TweenInfo.new(0.5)
		local goals = {BackgroundColor3 = originalColor, Transparency = 1}
		local tween = TweenService:Create(button, info, goals)
		tween:Play()
	end
	local function setDragColor()
		local info = TweenInfo.new(0.5)
		local goals = {BackgroundColor3 = dragColor, Transparency = 0.75}
		local tween = TweenService:Create(button, info, goals)
		tween:Play()
	end
	button.MouseEnter:Connect(function()
		setDragColor()
	end)
	button.MouseLeave:Connect(function()
		resetColor()
	end)
	button.MouseButton1Click:Connect(function()
		local info = TweenInfo.new(0.5)
		local goals = {BackgroundColor3 = clickColor, Transparency = 0.75}
		local tween = TweenService:Create(button, info, goals)
		tween:Play()
		isClicked = true
		if isOpen then
			func1()
			isOpen = false
		else
			func2()
			isOpen = true
		end
		wait(0.3)
		if not input.TouchEnabled then
			resetColor()
		end
	end)
	-- Handling touch input for mobile devices
	input.InputChanged:Connect(function(input, isProcessed)
		if isClicked and input.UserInputType == Enum.UserInputType.Touch and not isProcessed then
			setDragColor()
		end
	end)
	input.InputEnded:Connect(function(input)
		if isClicked and input.UserInputType == Enum.UserInputType.Touch then
			resetColor()
		end
	end)
end
local Holder = module.CreateUI()
function func1()
    print("Open")
end
function func2()
    print("Close")
end
module:NewButton(Holder, 15397687044, func1, func2) --id image
----------------------------------------------------------------------------------------------------------------------------------Function

